<div class="todo-card">
  <p style="color: green">
    <%= notice %>
  </p>
  <div class="title">
    <h1>Todo lists</h1>
  </div>
  <div class="todolist">
    <table>
      <tr>
        <th>Title</th>
        <th>Description</th>
        <th>Text</th>
        <th>Show this todo list</th>
      </tr>
      <div id="todo_lists">
        <% @todo_lists.each do |todo_list| %>
          <%= render todo_list %>
            <% end %>
    </table>
      </div>
</div>
</div>